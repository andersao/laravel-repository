{
    "name": "prettus/laravel-repository",
    "description": "Repositories to abstract the database layer",
    "authors": [
        {
            "name": "Anderson Andrade",
            "email": "contato@andersonandra.de"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "prettus/laravel-validation": "dev-master"
